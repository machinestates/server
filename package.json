{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "test": "jest --verbose --coverage --detectOpenHandles",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:migrate:undo": "npx sequelize-cli db:migrate:undo",
    "db:seed:all": "npx sequelize-cli db:seed:all",
    "db:reset": "npx sequelize-cli db:migrate:undo:all && npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all",
    "deploy": "eb deploy"
  },
  "dependencies": {
    "@aws-sdk/client-polly": "^3.363.0",
    "@metaplex-foundation/js": "^0.19.4",
    "@solana/spl-token": "^0.3.4",
    "@solana/web3.js": "1.56",
    "bcrypt": "^5.1.0",
    "check-password-strength": "^2.0.7",
    "cloudinary": "^1.37.3",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^16.3.1",
    "express": "~4.16.1",
    "hbs": "~4.0.4",
    "http-errors": "~1.6.3",
    "is-email": "^1.0.2",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "morgan": "~1.9.1",
    "mysql2": "^3.4.0",
    "openai": "^3.3.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "redis": "^4.6.7",
    "sequelize": "^6.32.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "aws-sdk-client-mock": "^3.0.0",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "sequelize-cli": "^6.6.1",
    "supertest": "^6.3.3"
  }
}
